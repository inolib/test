{
  "name": "@inolib/api",
  "version": "0.0.0",
  "description": "INOLIBâ€™s GraphQL API",
  "type": "module",
  "scripts": {
    "build": "vercel build",
    "build:api": "vite build",
    "build:types": "pnpm run build:types.gql && pnpm run build:types.prisma",
    "build:types.gql": "vite-node ./src/modules/prebuild.ts && graphql-codegen && vite-node ./src/modules/postbuild.ts",
    "build:types.prisma": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "vite-node ./prisma/seed.ts",
    "db:studio": "prisma studio --browser none",
    "deploy": "vercel deploy --prebuilt",
    "dev": "vercel dev",
    "format": "pnpm run format:prettier && pnpm run format:jsdoc && pnpm run format:packagejson && pnpm run format:prisma && pnpm run format:sort-imports",
    "format:jsdoc": "prettier --config ./.config/prettier/.prettierrc.jsdoc.cjs --write ./**/*.cjs ./**/*.ts",
    "format:packagejson": "prettier --config ./.config/prettier/.prettierrc.packagejson.cjs --write ./package.json",
    "format:prettier": "prettier --write .",
    "format:prisma": "prettier --config ./.config/prettier/.prettierrc.prisma.cjs --write ./**/*.prisma",
    "format:sort-imports": "prettier --config ./.config/prettier/.prettierrc.sort-imports.cjs --write ./**/*.ts",
    "lint": "pnpm run lint:eslint && pnpm run lint:tsc",
    "lint:eslint": "eslint --ext .cjs,.ts .",
    "lint:tsc": "tsc",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@envelop/graphql-modules": "^4.0.6",
    "@prisma/client": "^4.13.0",
    "express": "next",
    "graphql": "^16.6.0",
    "graphql-modules": "^2.1.2",
    "graphql-scalars": "^1.21.3",
    "graphql-yoga": "^3.9.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.3.0",
    "@graphql-codegen/graphql-modules-preset": "^3.1.2",
    "@graphql-codegen/typescript": "^3.0.3",
    "@graphql-codegen/typescript-resolvers": "^3.2.0",
    "@graphql-eslint/eslint-plugin": "^3.18.0",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@rollup/plugin-graphql": "^2.0.3",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.12",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitest/coverage-c8": "^0.30.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "glob": "^10.2.1",
    "graphql-config": "^4.5.0",
    "prettier": "^2.8.7",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "prettier-plugin-prisma": "^4.12.0",
    "prisma": "^4.13.0",
    "typescript": "^5.0.4",
    "vercel": "^28.20.0",
    "vite": "^4.3.1",
    "vite-node": "^0.30.1",
    "vite-plugin-node": "^3.0.2",
    "vitest": "^0.30.1"
  }
}
